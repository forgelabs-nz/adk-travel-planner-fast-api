# Google ADK Multi-Agent System Configuration
# Copy this file to .env and configure as needed
# All settings have sensible defaults - only override what you need

# ==============================================================================
# AI Model Configuration
# ==============================================================================

# Gemini model to use (default: gemini-2.5-flash)
MODEL=gemini-2.5-flash

# ==============================================================================
# Server Configuration
# ==============================================================================

# Server host and port
HOST=0.0.0.0
PORT=8010

# Environment: development or production
ENVIRONMENT=development

# ==============================================================================
# Database Configuration
# ==============================================================================

# Session database URL
# Development: Use SQLite
SESSION_DB_URL=sqlite:///./sessions.db

# Production: Use PostgreSQL/Cloud SQL
# SESSION_DB_URL=postgresql://user:pass@host:5432/dbname

# ==============================================================================
# CORS Configuration
# ==============================================================================

# Allowed origins for CORS (comma-separated)
# Use * for development, specific origins for production
ALLOWED_ORIGINS=*

# ==============================================================================
# Google Cloud Configuration
# ==============================================================================

# Google Cloud project (optional - uses gcloud config by default)
# Only set if you want to override your gcloud default project
# GOOGLE_CLOUD_PROJECT=your-project-id

# Google Cloud region (default: us-central1)
GOOGLE_CLOUD_LOCATION=us-central1

# Enable Google Cloud Logging (optional, default: false)
ENABLE_CLOUD_LOGGING=false
